[begin] (Fluxy)
  [encoding] {UTF-8}
    [exec] (Chrome)   {google-chrome}
    [exec] (Explorer) {nautilus / &}
    [exec] (Firefox)  {firefox}
    [exec] (Sublime)  {sublime}
    [exec] (Terminal) {gnome-terminal}

    [separator]

    [commanddialog] (Command)
    [config]        (Configure)
    [exec]          (About)     {(fluxbox -v; fluxbox -info | sed 1d) | xmessage -file - -center}
    [exec]          (Lock)      {xscreensaver-command -lock}
    [exec]          (Run)       {fbrun}
    [restart]       (Restart)
    [submenu]       (Skins)
      [stylesdir] (~/.fluxbox/styles)
    [end]

    [separator]

    [exit]    (Exit)
  [endencoding]
[end]
